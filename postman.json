											"    method: 'GET',",
											"}, function (err, res) {",
											"    if(err){",
											"",
											"        consle.error(err);",
											"",
											"    }else {",
											"",
											"        let data = res.json();",
											"        let key = Math.floor(Math.random()*data.length);",
											"        let item = data[key];",
											"",
											"        pm.variables.set(\"_id\", item._id);",
